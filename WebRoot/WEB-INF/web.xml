<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>GeneralTest</servlet-name>
    <servlet-class>test.GeneralTest</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddArgument</servlet-name>
    <servlet-class>action.AddArgument</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>GeneralTest</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddArgument</servlet-name>
    <url-pattern>/servlet/AddArgument</url-pattern>
  </servlet-mapping>

</web-app>
